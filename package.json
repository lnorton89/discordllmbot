{
  "name": "discordllmbot",
  "version": "1.0.0",
  "type": "module",
  "main": "bot/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lnorton89/discordllmbot.git"
  },
  "scripts": {
    "build": "docker-compose build",
    "dev": "docker-compose up -d bot api db pgadmin dashboard",
    "dev:container": "nodemon bot/index.js",
    "dev:down": "docker-compose down",
    "docs": "npm --prefix ./docs run dev",
    "start": "node bot/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.4",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}